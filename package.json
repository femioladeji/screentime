{
  "name": "screentime",
  "version": "5.0.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run build2 && npm run move",
    "build2": "webpack --entry /src/assets/js/background.js --mode=production --output-path ./dist/js/background",
    "move": "copyfiles manifest.json dist && copyfiles -f \"src/assets/images/*.png\" \"dist/images\"",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chart.js": "^2.9.4",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.4.0",
    "vue-router": "^3.0.1",
    "vue-select": "^3.13.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^27.2.2",
    "copyfiles": "^2.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^4.9.1"
  }
}
